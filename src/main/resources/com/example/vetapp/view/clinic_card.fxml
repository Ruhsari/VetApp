<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="149.0" prefWidth="231.0" spacing="5" style="-fx-padding: 20; -fx-background-color: #FFFFFF; -fx-background-radius: 20;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vetapp.controller.ClinicCardController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <Label fx:id="nameLabel" prefHeight="20.0" prefWidth="226.0" styleClass="label-medium" text="Name of clinic" textFill="#0a1c40">
      <font>
         <Font name="Poppins Medium" size="13.0" />
      </font>
   </Label>
   <Line endX="100.0" startX="-100.0" stroke="#d9dce5" strokeWidth="0.3" />
    <Label fx:id="addressLabel" styleClass="label-regular" text="Address" textFill="#0a1c40">
      <font>
         <Font name="Poppins Regular" size="12.0" />
      </font>
   </Label>
    <Label fx:id="ratingLabel" styleClass="label-regular" text="Rating " textFill="#0a1c40">
      <font>
         <Font name="Poppins Regular" size="12.0" />
      </font>
   </Label>
    <Label fx:id="servicesLabel" styleClass="label-regular" text="Services" textFill="#0a1c40">
      <font>
         <Font name="Poppins Regular" size="12.0" />
      </font>
   </Label>
</VBox>
