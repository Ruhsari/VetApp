<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5" style="-fx-padding: 10; -fx-background-color: #FFFFFF; -fx-border-radius: 10; -fx-background-radius: 10;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vetapp.controller.ClinicCardController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <Label fx:id="nameLabel" styleClass="label-bold" text="Название клиники" textFill="#615ca2">
      <font>
         <Font name="Montserrat SemiBold" size="12.0" />
      </font></Label>
    <Label fx:id="addressLabel" styleClass="label-normal" text="Адрес" />
    <Label fx:id="ratingLabel" styleClass="label-normal" text="Рейтинг" />
    <Label fx:id="servicesLabel" styleClass="label-normal" text="Услуги" />
</VBox>
