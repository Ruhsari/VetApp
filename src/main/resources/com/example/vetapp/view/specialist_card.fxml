    <Label fx:id="nameLabel" style="-fx-font-family: 'Montserrat'; -fx-font-weight: bold; -fx-font-size: 14px; -fx-text-fill: #0A1C40;" />
    <Label fx:id="specializationLabel" style="-fx-font-family: 'Montserrat'; -fx-font-size: 12px; -fx-text-fill: #516691;" />
</VBox>